{
  "buttons": [
    "html",
    "formatting",
    "bold",
    "italic",
    "unorderedlist",
    "orderedlist",
    "link"
  ],
  "plugins": [
    "fullscreen",
    "alignment",
    "properties",
    "specialchars",
    "beyondgrammar",
    "textdirection",
    "variable",
    "limiter",
    "linkattr",
    "counter"
  ],
  "linkAttrs": [
    {
      "label": "Class",
      "attr": "class",
      "type": "enum",
      "options": [
        {
          "name": "Primary Button",
          "value": "button primary-button"
        },
        {
          "name": "Secondary Button",
          "value": "button secondary-button"
        }
      ]
    }
  ],
  "formattingAdd": {
    "inline-code": {
      "title": "Inline Code",
      "api": "module.block.format",
      "args": {
        "tag": "kbd",
        "type": "set"
      }
    },
    "small-text": {
      "title": "Small Text",
      "api": "module.block.format",
      "args": {
        "attr": {
          "class": "text_small"
        },
        "add": "toogle"
      }
    },
    "remove-small-text": {
      "title": "Remove Small Text",
      "api": "module.block.format",
      "args": {
        "class": "text_small",
        "type": "remove"
      }
    }
  },
  "formattingHide": [
    "h1"
  ],
  "linkNewTab": true,
  "toolbarFixed": true
}